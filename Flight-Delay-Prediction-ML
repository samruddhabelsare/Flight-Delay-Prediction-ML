    "# Select only the columns needed for modeling\n",
    "features = ['month', 'day_of_week', 'op_unique_carrier', 'origin', 'dest', 'crs_dep_time', 'distance', 'delayed']\n",
    "df_model = df[features].dropna()\n",
    "\n",
    "# Now, split into X (features) and y (target)\n",
    "X = df_model[['month', 'day_of_week', 'op_unique_carrier', 'origin', 'dest', 'crs_dep_time', 'distance']]\n",
    "y = df_model['delayed']\n",
